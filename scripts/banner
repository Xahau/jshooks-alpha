#!/usr/bin/env bash

echo -e '             _                    # '"${RED}   ▐▄▄▄.▄▄ ·   ${PURPLE}   ▄ .▄            ▄ •▄ .▄▄ ·                ${END}"
echo -e '  __  ____ _| |__   __ _ _   _    # '"${RED}    ·██▐█ ▀.   ${PURPLE}  ██▪▐█▪     ▪     █▌▄▌▪▐█ ▀.                ${END}"
echo -e '  \ \/ / _` | "_ \ / _` | | | |   # '"${RED}  ▪▄ ██▄▀▀▀█▄  ${PURPLE}  ██▀▐█ ▄█▀▄  ▄█▀▄ ▐▀▀▄·▄▀▀▀█▄               ${END}"
echo -e '   >  < (_| | | | | (_| | |_| |   # '"${RED}  ▐▌▐█▌▐█▄▪▐█  ${PURPLE}  ██▌▐▀▐█▌.▐▌▐█▌.▐▌▐█.█▌▐█▄▪▐█               ${END}"
echo -e '  /_/\_\__,_|_| |_|\__,_|\__,_|   # '"${RED}   ▀▀▀• ▀▀▀▀   ${PURPLE}  ▀▀▀ · ▀█▄▀▪ ▀█▄▀▪·▀  ▀ ▀▀▀▀                ${END}"
